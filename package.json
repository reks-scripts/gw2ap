{
  "name": "gw2ap",
  "version": "1.0.9",
  "description": "Quick and easy searching and sorting of your Guild Wars 2 achievements",
  "author": "Ryan Knowles",
  "license": "MIT",
  "homepage": "https://gw2ap.com",
  "repository": {
    "url": "https://github.com/reks-scripts/gw2ap"
  },
  "scripts": {
    "start": "node app.js",
    "dev-server": "cross-env NODE_ENV=development nodemon ./app.js",
    "dev-client": "cross-env NODE_ENV=development webpack serve --config ./webpack.config.dev.js --progress --mode development",
    "dev": "npm-run-all -p dev-server dev-client",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --config ./webpack.config.build.js --progress --mode production"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@hapi/catbox-memory": "^5.0.0",
    "@hapi/hapi": "^20.0.1",
    "@hapi/inert": "^6.0.3",
    "boom": "^7.3.0",
    "bootstrap": "^4.5.2",
    "bootstrap-select": "^1.13.18",
    "datatables.net-bs4": "^1.10.22",
    "gasparesganga-jquery-loading-overlay": "^2.1.7",
    "hapi-gate": "^2.1.0",
    "jquery": "^3.5.1",
    "lodash": "4.17.21",
    "node-fetch": "^2.6.7",
    "popper.js": "^1.16.1",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "8.3.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "7.0.2",
    "css-loader": "7.1.2",
    "eslint": "^7.10.0",
    "file-loader": "6.1.0",
    "html-webpack-plugin": "^5.6.6",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^10.0.2",
    "style-loader": "1.2.1",
    "webpack": "^5.76.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "5.0.4",
    "webpack-merge": "^5.1.4"
  }
}
