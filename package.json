  "license": "MIT",
  "homepage": "https://gw2ap.com",
  "repository": {
    "url": "https://github.com/reks-scripts/gw2ap"
  },
  "scripts": {
    "start": "node app.js",
    "dev-server": "cross-env NODE_ENV=development nodemon ./app.js",
    "dev-client": "cross-env NODE_ENV=development webpack-dev-server --progress --mode development --config ./webpack.config.dev.js",
    "dev": "npm-run-all -p dev-server dev-client",
    "build": "webpack -p --progress --mode production --config ./webpack.config.build.js"
  },
